{
  "name": "express-api",
  "version": "1.0.0",
  "description": "Express API with Docker and TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "docker:dev": "docker compose --profile dev up --build",
    "docker:prod": "docker compose --profile prod up --build",
    "docker:down": "docker compose --profile dev down --remove-orphans",
    "db:migrate": "docker compose --profile dev exec api-dev npx prisma migrate dev",
    "db:migrate:create": "docker compose --profile dev exec api-dev npx prisma migrate dev --create-only",
    "db:migrate:deploy": "docker compose --profile dev exec api-dev npx prisma migrate deploy",
    "db:reset": "docker compose --profile dev exec api-dev npx prisma migrate reset",
    "db:generate": "docker compose --profile dev exec api-dev npx prisma generate",
    "db:studio": "docker compose --profile dev exec api-dev npx prisma studio --port 5555 --browser none",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "express",
    "api",
    "docker",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.0",
    "pg": "^8.16.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.19.3",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@vitest/coverage-v8": "^4.0.16",
    "prisma": "^7.1.0",
    "supertest": "^7.1.4",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16"
  }
}
